<p id="notice"><%= notice %></p>

<div class="jumbotron">
<h3>Organizations</h3>
</div>
<div class="container row">
    <% @organizations.each do |organization| %>
    <div class="col-md-3 col-xs-12 thumbnail">
        <%= organization.name %>
        <%= organization.about %>
        <%= organization.url %>
        <%= image_tag organization.logo.url(:thumb) %>
    </div>
    <% end %>
</div>
<br>

<%= link_to 'New Organization', new_organization_path %>
